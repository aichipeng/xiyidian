.footer_block {
    height: 116rpx;
}

.footer {
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    display: flex;
    color: #999999;
}

.footer .footer_item {
    padding: 10rpx 0;
    display: flex;
    flex: 1;
    flex-direction: column;
    align-items: center;
}

.footer .footer_item>image {
    width: 50rpx;
    height: 50rpx;
    display: block;
}