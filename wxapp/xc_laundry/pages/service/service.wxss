.nav {
    background: #fff;
    white-space: nowrap;
    height: 164rpx;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 100;
}

.nav .nav_item {
    width: 20%;
    text-align: center;
    color: #777777;
    display: inline-block;
    padding: 20rpx 0;
}

.nav .nav_item>image {
    width: 80rpx;
    height: 80rpx;
}

.nav_block {
    height: 164rpx;
}

.list {
    background: #fff;
    margin-top: 10rpx;
}

.list .item {
    padding: 20rpx;
    display: flex;
    align-items: center;
}

.list .item:nth-child(2n) {
    background: #f9f9f9;
}

.list .item+.item {
    border-top: #ddd 2rpx dashed;
}

.list .item>image {
    width: 50rpx;
    height: 50rpx;
}

.list .item>view:nth-child(2) {
    width: 40%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list .item>view:nth-child(3) {
    flex: 1;
    color: #999999;
}

.list .item .item_total {
    display: flex;
    align-items: center;
}

.list .item .item_total>view:nth-child(1) {
    color: #aaa;
    border: #aaa 2rpx solid;
    border-radius: 100%;
    width: 40rpx;
    height: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-right: 10rpx;
}

.list .item .item_total>view:nth-child(3) {
    color: #fff;
    border: #12b8f6 2rpx solid;
    border-radius: 100%;
    width: 40rpx;
    height: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #12b8f6;
    margin-left: 10rpx;
}

.list .item .price {
    color: #12b8f6;
}

.list2 {
    overflow: hidden;
}

.list2 .item {
    width: 32vw;
    background: #fff;
    padding: 2vw;
    margin-top: 10rpx;
    box-sizing: border-box;
    float: left;
    position: relative;
}

.list2 .item:nth-child(3n-1) {
    margin-left: 2vw;
}

.list2 .item:nth-child(3n) {
    margin-left: 2vw;
}

.list2 .item>image {
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto;
    display: block;
}

.list2 .item .item_r {
    width: 100%;
    text-align: center;
    line-height: 4vw;
}

.list2 .item .item_r>view:nth-child(1) {
    font-size: 4vw;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list2 .item .item_r>view:nth-child(2) {
    color: #999;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list2 .item .item_total {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 10rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    background: #ddf7ff;
    width: 100%;
    height: 60rpx;
    color: #fff;
}

.list2 .item .item_total>view:nth-child(1) {
    color: #fff;
    border-radius: 100%;
    width: 40rpx;
    height: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-right: 30rpx;
    border: #99e5ff 2rpx solid;
    background: #99e5ff;
}

.list2 .item .item_total>view:nth-child(3) {
    color: #fff;
    border: #99e5ff 2rpx solid;
    border-radius: 100%;
    width: 40rpx;
    height: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #99e5ff;
    margin-left: 30rpx;
}

.list2 .item .item_block {
    height: 60rpx;
}

.bottom_block {
    height: 100rpx;
}

.bottom {
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    display: flex;
    align-items: center;
    height: 100rpx;
    z-index: 1000;
}

.bottom>image {
    width: 60rpx;
    height: 60rpx;
    display: block;
    padding: 0 20rpx;
}

.bottom>view:nth-child(2) {
    color: #12b8f6;
    font-size: 4.5vw;
    flex: 1;
    padding: 0 40rpx;
}

.bottom>view:nth-child(3) {
    background: #12b8f6;
    color: #fff;
    text-align: center;
    padding: 0 40rpx;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 5vw;
}

.bottom .total {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    color: #fff;
    top: 10rpx;
    left: 80rpx;
    border-radius: 100%;
}

.menu {
    position: fixed;
    left: 0;
    bottom: 100rpx;
    z-index: 1000;
    background: #fff;
    width: 100%;
}

.menu .menu_top {
    display: flex;
    padding: 20rpx;
    color: #999;
    font-size: 4vw;
    border-bottom: #ddd 2rpx solid;
}

.menu .menu_top>view:nth-child(1) {
    width: 200rpx;
}

.menu .menu_top>view:nth-child(2) {
    flex: 1;
}

.menu .menu_content {
    height: 40vh;
    overflow: scroll;
}

.menu .menu_content .item {
    display: flex;
    padding: 20rpx;
    align-items: center;
    border-bottom: #ddd 2rpx solid;
}

.menu .menu_content .item icon {
    display: flex;
    align-items: center;
    justify-content: center;
}

.menu .menu_content .item .item_c {
    flex: 1;
    display: flex;
    padding: 0 40rpx;
}

.menu .menu_content .item .item_c>image {
    width: 100rpx;
    height: 100rpx;
}

.menu .menu_content .item .item_c .item_cc {
    margin-left: 20rpx;
    flex: 1;
}

.menu .menu_content .item .item_c .item_cc>view:nth-child(1) {
    font-size: 4vw;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 200rpx;
}

.menu .menu_content .item .item_c .item_cc>view:nth-child(2) {
    color: #999;
}

.menu .menu_content .item .item_r .menu_num {
    display: flex;
    align-items: center;
    border: #999999 2rpx solid;
    border-radius: 40rpx;
}

.menu .menu_content .item .item_r .menu_num>view {
    padding: 10rpx 20rpx;
}

.menu .menu_content .item .item_r .menu_num>view:nth-child(2) {
    color: #000;
}

.menu .menu_content .item .item_r .menu_num>view:nth-child(3) {
    font-size: 5vw;
}